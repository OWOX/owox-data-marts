{
  "name": "@owox/backend",
  "version": "0.14.0",
  "description": "OWOX Data Marts Backend - Full-stack data orchestration platform",
  "author": "OWOX",
  "license": "ELv2",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/OWOX/owox-data-marts#readme",
  "bugs": {
    "url": "https://github.com/OWOX/owox-data-marts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OWOX/owox-data-marts.git",
    "directory": "apps/backend"
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json",
    "dist/**/*.html",
    "dist/**/*.css",
    "dist/**/*.png",
    "dist/**/*.ico",
    "dist/**/*.svg"
  ],
  "scripts": {
    "build": "nest build",
    "build:clean": "shx rm -rf dist",
    "build:connectors": "npm run build -w @owox/connectors --prefix ../..",
    "build:dep": "npm run build:internal-helpers && npm run build:connectors && npm run build:idp-protocol",
    "build:idp-protocol": "npm run build -w @owox/idp-protocol --prefix ../..",
    "build:internal-helpers": "npm run build -w @owox/internal-helpers --prefix ../..",
    "prebuild": "npm run build:clean && npm run build:dep",
    "prepack": "npm run build",
    "prepublishOnly": "npm run lint",
    "format": "prettier --write \"**/*.{ts,json}\" --ignore-path ../../.prettierignore",
    "format:check": "prettier --check \"**/*.{ts,json}\" --ignore-path ../../.prettierignore",
    "start": "nest start",
    "start:dev": "nest start --watch --env-file ../../.env",
    "start:debug": "nest start --debug --watch --env-file ../../.env",
    "start:prod": "node dist/src/main",
    "serve": "npm run start:prod",
    "lint": "eslint . --config ./eslint.config.mjs",
    "lint:fix": "eslint . --fix --config ./eslint.config.mjs",
    "lint:md": "markdownlint-cli2 --config ../../.markdownlint-cli2.mjs",
    "lint:md:fix": "markdownlint-cli2 --config ../../.markdownlint-cli2.mjs --fix",
    "dev": "npm run start:dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrations:create-template": "ts-node commands/create-migration-template-command.ts",
    "dump:create": "ts-node commands/create-dump-command.ts",
    "dump:apply": "ts-node commands/apply-dump-command.ts"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.839.0",
    "@aws-sdk/client-s3": "^3.840.0",
    "@google-cloud/bigquery": "^8.1.0",
    "@google-cloud/pubsub": "^4.0.7",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@owox/connectors": "*",
    "@owox/idp-protocol": "*",
    "@owox/internal-helpers": "*",
    "@sendgrid/mail": "^8.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-spawn": "^7.0.6",
    "env-paths": "^3.0.0",
    "googleapis": "^150.0.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.0",
    "juice": "^11.0.3",
    "luxon": "^3.6.1",
    "mysql2": "^3.14.1",
    "nestjs-cls": "^6.0.1",
    "reflect-metadata": "^0.2.2",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rxjs": "^7.8.1",
    "snowflake-sdk": "^2.3.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24",
    "typeorm-transactional": "^0.5.0",
    "zod": "^3.25.64",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.9",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.2",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
