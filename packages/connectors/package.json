{
  "name": "@owox/connectors",
  "version": "0.17.0",
  "description": "Connectors and storages for different data sources",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "dev": "vite build --config vite.config.js",
    "dev:watch": "node watcher.config.js",
    "build": "vite build --config vite.config.js",
    "clean": "shx rm -rf dist",
    "prebuild": "npm run clean",
    "analyze": "npm run build -- --mode analyze",
    "prepack": "npm run build",
    "prepublishOnly": "npm run lint",
    "lint": "eslint . --config ./eslint.config.js",
    "lint:fix": "eslint . --fix --config ./eslint.config.js",
    "lint:md": "markdownlint-cli2 --config ../../.markdownlint-cli2.mjs",
    "lint:md:fix": "markdownlint-cli2 --config ../../.markdownlint-cli2.mjs --fix",
    "format": "prettier --write \"**/*.{js,mjs,json}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,json}\""
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs"
  ],
  "bin": {
    "owox-connector-runner": "./dist/connector-runner.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./runner": {
      "require": "./dist/connector-runner.cjs"
    }
  },
  "dependencies": {
    "@aws-sdk/client-athena": "3.810.0",
    "@aws-sdk/client-redshift-data": "3.810.0",
    "@aws-sdk/client-s3": "3.810.0",
    "@aws-sdk/lib-storage": "3.810.0",
    "@databricks/sql": "^1.12.0",
    "@google-cloud/bigquery": "^8.0.0",
    "adm-zip": "0.5.16",
    "snowflake-sdk": "2.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@owox/prettier-config": "*",
    "chokidar": "^3.5.0",
    "eslint-config-prettier": "^10",
    "fs-extra": "^11",
    "glob": "^13",
    "globals": "^16",
    "vite": "^6.3.5"
  }
}
